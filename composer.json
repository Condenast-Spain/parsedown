{
    "name": "condenast-spain/parsedown",
    "description": "Parser for Markdown.",
    "keywords": ["markdown", "parser"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CondeNast Spain",
            "homepage": "http://www.cnworld.es/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35"
    },
    "autoload": {
        "psr-0": {"Parsedown": ""}
    },
    "autoload-dev": {
        "psr-0": {
            "TestParsedown": "test/",
            "ParsedownTest": "test/",
            "CommonMarkTest": "test/",
            "CommonMarkTestWeak": "test/"
        }
    }
}
